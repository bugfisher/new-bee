</head>

<body>

  <!-- Navigation -->
  <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top">
    
    
    
      <a class="navbar-brand" href="#" style="font-family:'Kaushan Script', cursive"><font color="#7dce94"  size="7px">newbiee</font></a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarResponsive">

      <ul class="navbar-nav ml-auto">

      <li class="nav-item">
        
         <a class="nav-link" href="/index"  id="index" style="font-family:'Kaushan Script', cursive" data-toggle="tooltip" title="home"><font size="5px" color="#7dce94">Home</a></font>
          </li>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;

      <li class="nav-item">
        
         <a class="nav-link" href="/"  id="logout" style="font-family:'Kaushan Script', cursive" data-toggle="tooltip" title="bye!"><font size="5px" color="#7dce94">Logout</a></font>
          </li>


      
    </ul>
  </div> 
</nav>
<div style="background-color:#f6f5f3">

    <br><br><br><br>

    <!-- Page Content -->
    <div class="container">

        <!-- Heading Row -->
        <div class="row align-items-center my-5">
            <div class="col-lg-7">
                &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                <% userinfo.forEach((user, index) => { %>
                <img src="/assets/img/<%= user.ProfilePic %>" width="300" height="300" class="rounded-circle" alt="">
            </div>
            <!-- /.col-lg-8 -->
            
            

                <div class="col-lg-5">
                    <font size="10">
                        <p class="font-weight-light" size="20">
                            <b><%= user.Name %></b>
                        </p>
                    </font>
                    <h4 class="font-weight-light">
                        <i class="fa fa-envelope" style="font-size:24px"></i>
                        <%= user.Email %>
                            <h4 class="font-weight-light">
                            </h4>
                            <h4 class="font-weight-light">
                                <i class="fa fa-phone" style="font-size:24px"></i>
                                <%= user.Contact %>
                            </h4>


                </div>

                <% }) %>
                    <!-- /.col-md-4 -->
        </div>
        <!-- /.row -->
        <% if(groups.length > 0){ %>

        <i> <font color="green" style="font-family:'Kaushan Script', cursive"><center><h1>Groups Joined</i></h1></center></font> <br>  
        <%}else{ %>
        <i> <font color="green" style="font-family:'Kaushan Script', cursive"><center><h1>No Groups Joined Yet</i></h1></center></font> <br>  
        <% } %>

<div class="row text-center">
<% groups.forEach((group, index) => { %>

<div class="col-lg-3 col-md-6 mb-4">
            <div class="card h-100">
            <img class="card-img-top" src="/assets/img/<%=group.G_Image%>" class="rounded-circle player-img" height="200px" width="200px" alt="Sports">
            <div class="card-body">
                <h4 class="card-title"><%= group.GroupName %></h4>
                <p class="card-text"><%= group.GroupDescription %></p>
                
            </div>
            <div class="card-footer">
                <a href="/group_event/<%=group.G_Id%>" class="btn btn-outline-success">See More</a>
            </div>
            </div>
        </div>



<% }) %>
</div>

 
    </div> 
        


        <!-- Footer -->
        <footer class="py-5 bg-dark">
                <p class="m-0 text-center text-white" style="font-family:'Kaushan Script', cursive">Copyright &copy; Your Website 2019</p>
            <!-- /.container -->
        </footer>

        <!-- Bootstrap core JavaScript -->
        <script src="vendor/jquery/jquery.min.js"></script>
        <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

</body>

</html>