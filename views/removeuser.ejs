  <br><br><br><br><br><br>

  <center><h1 style="font-family:'Kaushan Script', cursive">List of Users</h1></center>
    <br><br>

 <% if (result.length > 0) { %>
                
    <table class="table">
    <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Name</th>
      <th scope="col">User Name</th>
      <th scope="col">Enail</th>
      <th scope="col">Contact</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>

  <% result.forEach((user, index) => { %>
    <tr>
      <td><%= user.U_Id %></td>
      <td><%= user.Name %></td>
      <td><%= user.Username %></td>
      <td><%= user.Email %></td>
      <td><%= user.Contact %></td>
      <td><a href="/RemoveUser/<%= user.U_Id %>" ><font color="red" size="4px">&#10008;</font>&nbsp;Remove</a></td>
    </tr>

  <% }) %>
    </tbody>
</table>
  <% } else { %>

        <h2 style="font-family:'Kaushan Script', cursive">No Users Present</h2>
       <% } %>


